<?xml version='1.0' encoding='utf-8'?>
<widget id="com.carlos.cadernao" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AnísioCadernão</name>
    <description>
        Caderno de questões de diversas disciplinas para a EEETEPA Anísio Teixeira
    </description>
    <author email="gigakarlinhos@gmail.com" href="http://www.maticatecno.com.br">
        Carlos Roberto
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="28" />
</widget>
